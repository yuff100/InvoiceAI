{
  "version": 2,
  "functions": {
    "api/ocr/process.ts": {
      "maxDuration": 30
    },
    "api/tesseract/ocr.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}