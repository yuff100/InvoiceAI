{
  "version": 2,
  "functions": {
    "api/ocr/process.ts": {
      "maxDuration": 30
    },
    "api/oa/push.ts": {
      "maxDuration": 10
    },
    "api/tesseract/ocr.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "QINIU_ACCESS_KEY": "@qiniu-access-key",
    "QINIU_SECRET_KEY": "@qiniu-secret-key",
    "QINIU_BUCKET": "@qiniu-bucket",
    "ZHIPU_API_KEY": "@zhipu-api-key"
  }
}