{
  "functions": {
    "api/qiniu/signature.ts": {
      "maxDuration": 30
    },
    "api/ocr/process.ts": {
      "maxDuration": 120,
      "includeFiles": "lib/**/*.ts"
    },
    "api/tesseract/ocr.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}